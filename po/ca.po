#: data/dconf/org.freedesktop.ibus.gschema.xml:265
msgid "Default language for emoji dictionary"
msgstr "Idioma predeterminat per al diccionari emoji"

#: data/dconf/org.freedesktop.ibus.gschema.xml:266
